{
  "name": "@mscs/environment",
  "version": "0.1.1-DEV",
  "description": "A library to load .env files and interact with the environment",
  "author": "Jason Schilling <jschilling@media-service.com>",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:media-service-dev/environment.git"
  },
  "keywords": [
    "dotenv",
    ".env",
    "environment"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "build:dev": "webpack --config webpack.config.js --mode=development --display-modules",
    "build:watch": "webpack --config webpack.config.js --watch --mode=development",
    "lint": "eslint ./src/ ./__tests__/",
    "test": "jest",
    "docs": "typedoc --listInvalidSymbolLinks src/"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "docs/",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@mscs/eslint-config": "^0.2.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.20",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^12.19.16",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "mock-fs": "^4.13.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.3",
    "webpack": "^5.20.1",
    "webpack-cli": "^4.5.0"
  }
}
