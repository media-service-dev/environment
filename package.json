{
  "name": "@mscs/environment",
  "version": "0.1.2-DEV",
  "description": "A library to load .env files and interact with the environment",
  "author": "Jason Schilling <jschilling@media-service.com>",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:media-service-dev/environment.git"
  },
  "keywords": [
    "dotenv",
    ".env",
    "environment"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "build:dev": "webpack --config webpack.config.js --mode=development",
    "build:watch": "webpack --config webpack.config.js --watch --mode=development",
    "lint": "eslint ./src/ ./__tests__/",
    "test": "jest",
    "docs": "typedoc src/"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "docs/",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@mscs/eslint-config": "^0.2.2",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.3",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.11.14",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "jest-each": "^27.4.2",
    "mock-fs": "^5.1.2",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
